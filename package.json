{
    "name": "smart-console",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "sandbox/*",
        "shared/*",
        "package/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "namespace": "@smart-console",
    "devDependencies": {
        "eslint-config-custom-next": "workspace:*",
        "prettier": "latest",
        "turbo": "latest"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "packageManager": "pnpm@7.17.1"
}
